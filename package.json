    "test-basic": "NODE_ENV=testing jest --forceExit --detectOpenHandles --verbose"
  },
  "homepage": "https://github.com/mpicollo-wolox/typescript-base",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.6",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^9.2.2",
    "ts-node-dev": "1.0.0-pre.44",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  }
}
