  "description": "Example base for typescript projects",
  "exports": "./index.ts",
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "publish:npm": "rm -rf dist && mkdir dist &&  babel src/components -d dist --copy-files",
