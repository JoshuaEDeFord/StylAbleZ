{
  "name": "stylablez",
  "version": "1.0.0",
  "description": "StylAbleZ Components",
  "repository": "git+ssh://git@github.com:JoshuaEDeFord/StylAbleZ.git#add-examples",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "sourceMap": true,
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "typescript": "~3.8.2",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.6",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^9.2.2",
    "ts-node-dev": "1.0.0-pre.44",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
  }
}
